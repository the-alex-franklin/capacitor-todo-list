{
	"version": "0.2.0",
	"configurations": [
		{
			"request": "launch",
			"name": "Launch Program",
			"type": "node",
			"runtimeExecutable": "deno",
			"runtimeArgs": [
				"run",
				"--inspect-wait",
				"--unstable",
				"--watch",
				"${workspaceFolder}/src/server.ts"
			],
			"attachSimplePort": 9229,
			"console": "integratedTerminal"
		}
	]
}
